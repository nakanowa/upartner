</script>\r        <script type=\"text/javascript\" src=\"/js/google-api.js\"></script>\r\r\r        <script type=\"text/javascript\">\r            \r            \r        $(function() {\r            $('#newCalButton').click(function() {\r                $(\"#loading\").show();\r                $.get ('Zend/index.php',\r                        null,\r                        function () {\r                            $(\"#gCalendar\").html('<iframe src=\"https://www.google.com/calendar/embed?src=upartner0000%40gmail.com&ctz=Asia/Tokyo\" style=\" border:solid 1px #777 \" width=\"580\" height=\"450\" frameborder=\"0\" scrolling=\"no\" id=\"CalFrame\"></iframe>');\r                            \r                            $(\"#loading\").fadeOut(function() {\r                            });\r                        });\r                });\r        });\r        \r        //googleカレンダー読み込み\r        google.load(\"gdata\", \"2\");\r    \r        var myService = null;\r        var query = null;\r//        var feedUrl = \"http://www.google.com/calendar/feeds/97c6dd7p0ag1o3egf4dhundrds%40group.calendar.google.com/public/full\";\r        var feedUrl = \"https://www.google.com/calendar/feeds/97c6dd7p0ag1o3egf4dhundrds%40group.calendar.google.com/private/full\";\r    \r        google.setOnLoadCallback(initialize);\r    \r        function initialize(){\r            google.gdata.client.init(handleError);\r            myService = new google.gdata.calendar.CalendarService(\"u-partner-calendar\");\r\t\t\tquery = new google.gdata.calendar.CalendarEventQuery(feedUrl);\r\t\t\tquery.setOrderBy(google.gdata.calendar.CalendarEventQuery.ORDERBY_START_TIME);\r\t\t\tquery.setSortOrder(google.gdata.calendar.CalendarEventQuery.SORTORDER_ASCENDING);\r\t\t\tquery.setSingleEvents(true);\r\t\t\tquery.setMaxResults(10);\r        }\r        \r        function getMyFeed(){\r            var date1 = document.getElementById(\"start\").value;\r            var date2 = document.getElementById(\"end\").value;\r            var start = new google.gdata.DateTime.fromIso8601(date1);\r            var end = new google.gdata.DateTime.fromIso8601(date2);\r            query.setMinimumStartTime(start);\r            query.setMaximumStartTime(end);\r            myService.getEventsFeed(query, callback, handleError);\r        }\r    \r        function callback(result){\r            var entries = result.feed.entry;\r            var res = \"\";\r            for (var i = 0; i < entries.length; i++) {\r                var entry = entries[i];\r                var title = entry.getTitle().getText();\r                res += title + '<br />';\r                \r                var times = entry.getTimes();\r                if(times.length > 0) {\r                    var time = times[0].getStartTime();\r                    res += \"(\" + time.getDate() + \")<br />\";\r                }\r                \r                var link = entry.getHtmlLink();\r                if(link != null) {\r                    var href = link.getHref();\r                    res += '<a href=\"' + href + '\">LINK</a><br />';\r                }\r                res += \"<br />\";\r            }\r            document.getElementById(\"cal_panel\").innerHTML = '<h3>[Googleカレンダー 検索結果]</h3>' + res;\r        }\r    \r        function handleError(error) {\r            alert(error);\r        }\r        //ここまで googleカレンダー読み込み\r\r\r\r        //google検索\r        var controll = null;\r            \r        google.load('search', '1');\r            \r        google.setOnLoadCallback(function() {\r            controll = new google.search.SearchControl();\r            var option = new google.search.SearcherOptions();\r            option.setExpandMode(google.search.SearchControl.EXPAND_MODE_OPEN);\r                \r            var drawOption = new google.search.DrawOptions();\r//                drawOption.setDrawMode(google.search.SearchControl.DRAW_MODE_TABBED);\r            drawOption.setDrawMode(google.search.SearchControl.DRAW_MODE_LINEAR);\r            controll.addSearcher(new google.search.WebSearch());\r            controll.addSearcher(new google.search.ImageSearch());\r            controll.addSearcher(new google.search.LocalSearch());\r            controll.draw(document.getElementById(\"search_panel\"));\r        });\r           \r           function doFind() {\r                var keyword = null;\r                var categoly = null;\r               \r                if($('input[name=keyword]').val() === ''){\r                   window.alert('キーワードが未入力です。');\r                   return;\r                }\r                \r//                if($('select[name=categolySelect]').val() === 'default'){\r//                   window.alert('カテゴリが未選択です。');\r//                   return; \r//                }\r                \r                keyword = document.getElementById(\"keyword\");\r                categoly = document.getElementById(\"categolySelect\");\r                \r                var str = keyword.value + ' '+ categoly.value;\r                var hpStr = keyword.value;\r                var gnStr = keyword.value + ' ' + '東京';\r\r                controll.execute(str);\r                \r                \r                //楽天API検索\r                // デベロッパーID\r                var developerId = '1033715658890127503';\r        \r        \r                // アクセス先のURLを作成\r                var url = 'http://api.rakuten.co.jp/rws/3.0/json?';\r                url += '&developerId=' + developerId;\r                url += '&keyword=' + encodeURIComponent(str);\r                url += '&operation=ItemSearch&version=2010-09-15&callBack=?';\r        \r        \r                // JSONPで情報を得る\r                $.getJSON(url, function(data) {\r                    var items = data.Body.ItemSearch.Items.Item;\r\r                    var html = '<p>【楽天】検索結果</p>';\r                \r                    for (var i = 0, j = items.length; i < 5; i++) {\r                        var item = items[i];\r                    \r                        // 商品名や画像のデータを元に、HTMLを組み立てる\r//                        html += '<p><a href=\"' + item.itemUrl + '\">';\r//                        html += '<img src=\"' + item.mediumImageUrl + '\" alt=\"' + item.itemName + '\" title=\"' + item.itemName + '\" /></a><br />';\r//                        html += '<a href=\"' + item.itemUrl + '\">' + item.itemName + '</a><br />';\r//                        html += item.itemPrice + '円<br />';\r//                        html += '<a href=\"' + item.shopUrl + '\">' + item.shopName + '</a></p>';\r\r                        html += '<p><a href=\"' + item.itemUrl + '\" target=\"_blank\">' + item.itemName + '</a></p>';\r      \r                        // IDが「search_result」の要素にHTMLを流し込む\r                        $('#rakuten_search_result').html(html);\r                    }\r                });\r                \r                \r                \r                //　Hot Pepper API検索\r                // デベロッパーID\r                var hpDeveloperId = 'dbba8b8db6df5745';\r        \r        \r                // アクセス先のURLを作成\r                var hpurl = 'http://webservice.recruit.co.jp/hotpepper/gourmet/v1/?';\r                hpurl += 'key=' + hpDeveloperId;\r                hpurl += '&keyword=' + encodeURIComponent(hpStr);\r                hpurl += '&format=jsonp&callback=?';\r                \r                \r                $.getJSON(hpurl, function(datahp) {\r                    \r                    \r                    var hpItems = datahp.results.shop;\r                    var hpHtml = '<p>【Hot Pepper】検索結果</p>';\r                \r                    for (var i = 0, j = hpItems.length; i < 10; i++) {\r                        var hpItem = hpItems[i];\r                    \r                        hpHtml += '<p><a href=\"' + hpItem.urls.pc + '\" target=\"_blank\">' + hpItem.name + '</a></p>';\r      \r                        // IDが「search_result」の要素にHTMLを流し込む\r                        $('#hotpepper_search_result').html(hpHtml);\r                    }\r                });\r                //ここまで　Hot Pepper API検索\r\r                \r                //　ぐるなび API検索\r                // デベロッパーID\r                var gnDeveloperId = '568ef12101de8e79e639a215a239bec0';\r        \r        \r        \r                // アクセス先のURLを作成\r                var gurunaviUrl = 'http://api.gnavi.co.jp/ver1/RestSearchAPI/?';\r//                var gurunaviUrl = 'http://api.gnavi.co.jp/ver1/RestSearchAPI/?keyid=568ef12101de8e79e639a215a239bec0&area=AREA110&pref=PREF13&id=g144600&sort=1'\r//                gurunaviUrl += 'keyid=' + gnDeveloperId;\r//                gurunaviUrl += '&freeword=' + encodeURIComponent(hpStr);\r                gurunaviUrl += 'keyid=568ef12101de8e79e639a215a239bec0&area=AREA110&pref=PREF13&sort=1';\r\r\r                $.ajax({\r                    crossDomain: true,\r                    dataType: \"xml\",\r                    url: \"http://api.gnavi.co.jp/ver1/RestSearchAPI/?keyid=568ef12101de8e79e639a215a239bec0&freeword=\" + encodeURIComponent(hpStr),\r                    contentType: 'application/json; charset=utf-8',\r                    type: \"get\",\r                    success: function (data) {\r                    alert(data.response.rest.name[1]);\r//                  console.log(data);\r                    }\r                });\r                \r//                var resGnaviUrl = xml2Json(gurunaviUrl, \"callBackX2J\");\r//                alert(callBackX2J.data.rest[\"赤坂　貸切　バー Bubble Bar\"][\"name_kana\"]);\r//                alert(resGnaviUrl);\r\r//                $.getJSON(resGnaviUrl, function(data) {\r                    \r//                    var hpItems = datahp.results.shop;\r//                    var hpHtml = '<p>【Hot Pepper】検索結果</p>';\r//                \r//                    for (var i = 0, j = hpItems.length; i < 5; i++) {\r//                        var hpItem = hpItems[i];\r//                    \r//                        hpHtml += '<p><a href=\"' + hpItem.urls.pc + '\" target=\"_blank\">' + hpItem.name + '</a></p>';\r//      \r//                        // IDが「search_result」の要素にHTMLを流し込む\r//                        $('#grunavi_search_result').html(hpHtml);\r//                    }\r//                });\r                \r                \r                \r            }\r        </script>\r        \r\r        <script type='text/javascript'>\r            \r            $(document).ready(function() {\r                \r                $('#external-events div.external-event').each(function() {\r                    \r                    // create an Event Object (http://arshaw.com/fullcalendar/docs/event_data/Event_Object/)\r                    // it doesn't need to have a start or end\r                    var eventObject = {\r                        title: $.trim($(this).text()) // use the element's text as the event title\r                    };\r                    \r                    // store the Event Object in the DOM element so we can get to it later\r                    $(this).data('eventObject', eventObject);\r                    \r                    // make the event draggable using jQuery UI\r                    $(this).draggable({\r                        zIndex: 999,\r                        revert: true,      // will cause the event to go back to its\r                        revertDuration: 0  //  original position after the drag\r                    });\r                    \r\t\t});\r                \r                \r\t\tvar date = new Date();\r\t\tvar d = date.getDate();\r\t\tvar m = date.getMonth();\r\t\tvar y = date.getFullYear();\r\t\t\r\t\tvar calendar = $('#calendar').fullCalendar({\r                    \r                    header: {\r                        left: 'prev,next today',\r                        center: 'title',\r                        right: 'month,agendaWeek,agendaDay'\r                    },\r                    selectable: true,\r                    selectHelper: true,\r                    select: function(start, end, allDay) {\r//                        var title = prompt('予定を入力して下さい:');\r//                        if (title) {\r//                            calendar.fullCalendar('renderEvent',\r//                            {\r//                                id : start,\r//                                title: title,\r//                                start: start,\r//                                end: end,\r//                                allDay: allDay\r//                            },\r//                            true // make the event \"stick\"\r//                        );\r//                        }\r                        entry_event(start, end, allDay);\r                        calendar.fullCalendar('unselect');\r                    },\r                    editable: true,\r                    events: [\r                        {\r                            id: 992,\r                            title: 'All Day Event',\r                            start: new Date(y, m, 1),\r                            'tooltip': '<div id=\"tooltip\">可変する文字列</div>'\r                        },\r                        {\r                            id: 993,\r                            title: 'Long Event',\r                            start: new Date(y, m, d-5),\r                            end: new Date(y, m, d-2)\r                        },\r                        {\r                            id: 999,\r                            title: 'Repeating Event',\r                            start: new Date(y, m, d-3, 16, 0),\r                            allDay: false\r                        },\r                        {\r                            id: 999,\r                            title: 'Repeating Event',\r                            start: new Date(y, m, d+4, 16, 0),\r                            allDay: false\r                        },\r                        {\r                            id: 996,\r                            title: 'Meeting',\r                            start: new Date(y, m, d, 10, 30),\r                            allDay: false\r                        },\r                        {\r                            id: 997,\r                            title: 'Lunch',\r                            start: new Date(y, m, d, 12, 0),\r                            end: new Date(y, m, d, 14, 0),\r                            allDay: false\r                        },\r                        {\r                            id: 998,\r                            title: 'Birthday Party',\r                            start: new Date(y, m, d+1, 19, 0),\r                            end: new Date(y, m, d+1, 22, 30),\r                            allDay: false\r                        },\r                        {\r                            id: 990,\r                            title: 'Click for Google',\r                            start: new Date(y, m, 28),\r                            end: new Date(y, m, 29),\r                            url: 'http://google.com/'\r                        }\r                    ],\r                    eventSources: [\r                        {\r                            url:'http://www.google.com/calendar/feeds/ja.japanese%23holiday%40group.v.calendar.google.com/public/full/',\r                            color: \"red\",\r                            success:function(events){\r                                $(events).each(function(){\r                                    this.url = null;\r                                });\r                            }\r                        }\r                    ],\r                    droppable: true, // this allows things to be dropped onto the calendar !!!\r                    drop: function(calDate, allDay) { // this function is called when something is dropped\r\t\t\t\r                        // retrieve the dropped element's stored Event Object\r                        var originalEventObject = $(this).data('eventObject');\r                        \r                        // we need to copy it, so that multiple events don't have a reference to the same object\r                        var copiedEventObject = $.extend({}, originalEventObject);\r                        \r                        // assign it the date that was reported\r                        copiedEventObject.start = calDate;\r                        copiedEventObject.allDay = allDay;\r                        copiedEventObject.id = new Date();\r                        \r                        \r                        // render the event on the calendar\r                        // the last `true` argument determines if the event \"sticks\" (http://arshaw.com/fullcalendar/docs/event_rendering/renderEvent/)\r                        $('#calendar').fullCalendar('renderEvent', copiedEventObject, true);\r                        \r                        // is the \"remove after drop\" checkbox checked?\r                        if ($('#drop-remove').is(':checked')) {\r                            // if so, remove the element from the \"Draggable Events\" list\r                            $(this).remove();\r                        }\r                        \r                    },\r                    eventDblClick:function(event, jsEvent){\r                        var title = prompt('予定を入力してください:', event.title);\r                        \r                        if(title && title!=\"\"){\r                            event.title = title;\r                            calendar.fullCalendar('updateEvent', event); //イベント（予定）の修正\r                        }\r//                        else{\r//                            calendar.fullCalendar(\"removeEvents\", event.id); //イベント（予定）の削除              \r//                        }\r                    },\r                    eventMouseover: function(calEvent, jsEvent) {\r                    $('body').prepend(calEvent.tooltip);\r\r                    xOffset = 30 + $('#tooltip').height();\r                    yOffset = -20;\r\r                    $('#tooltip')\r                    .css('top', (jsEvent.clientY - xOffset) + 'px')\r                    .css('left', (jsEvent.clientX + yOffset) + 'px')\r                    .fadeIn();\r                    },\r\r                    eventMouseout: function(calEvent, jsEvent) {\r                    $('#tooltip').remove();\r                    }\r                });\t\r            });\r            \r            $(\"#dialog-form\").dialog({\r                autoOpen: false,\r                height: 360,\r                width: 450,\r                modal: true,\r                buttons: {\r                    \"削除\": function() {\r                        remove_event(eventObj);\r                        },\r                    \"中止\": function() {\r                        $(this).dialog(\"close\");\r                        },\r                    \"貼付\": function() {\r                        if (validator()) {\r                            paste_event(eventObj)\r                        };\r                    },\r                    \"変更\": function() {\r                        if (validator()) {\r                    change_event(eventObj)\r                        };\r                    },\r                    \"登録\": function() {\r                        if (validator()) {\r                            post_event();\r                        };\r                    }\r                },\r        \r                close: function() {\r                    allFields.val(\"\").removeClass(\"ui-state-error\");\r                }\r            });\r\r\r            function entry_event(startDate, endDate, allDay) {\r                $(\"#ui-dialog-title-dialog-form\").text(\"イベント登録\");\r                $(\".ui-button\").eq(0).css(\"display\", \"none\");    // 削除ボタンを非表示。\r                $(\".ui-button\").eq(1).css(\"display\", \"inline\");    // 中止ボタンを表示。\r                $(\".ui-button\").eq(2).css(\"display\", \"none\");    // 貼付ボタンを非表示。\r                $(\".ui-button\").eq(3).css(\"display\", \"none\");    // 変更ボタンを非表示。\r                $(\".ui-button\").eq(4).css(\"display\", \"inline\").addClass(\"ui-state-focus\");    // 登録ボタンを表示。\r                $(\"input#start\").val($.fullCalendar.formatDate(startDate, 'yyyy-MM-dd HH:mm'));\r                $(\"input#end\").val($.fullCalendar.formatDate(endDate, 'yyyy-MM-dd HH:mm'));\r                $(\"input#reminder\").val($.fullCalendar.formatDate(startDate, 'yyyy-MM-dd HH:mm'));\r                $(\"input#reminder\").css(\"color\", \"gray\");\r        \r\r                document.getElementById(\"dialogDesc\").value = \"\";\r                $(\"#dialog-form\").dialog(\"open\");\r            };\r            \r            \r        </script>\r\r\r    </body>\r</html>\r"]],"start1":0,"start2":0,"length1":0,"length2":41002}]],"length":41002}
